{
	"manifest_version" : 2,
	"name" : "Titech Zoom URL",
	"version" : "1.0.2",
	"description": "講義のzoomリンクを自動で取得します",
	"permissions": [
		"storage",
		"https://portal.nap.gsic.titech.ac.jp/*",
		"https://kyomu2.gakumu.titech.ac.jp/*",
		"https://t2schola.titech.ac.jp/*",
		"https://secure.ocw.titech.ac.jp/*"
	],
	"icons": {
		"256": "icon.png"
	},
	"browser_action": {
		"default_popup": "popup/index.htm"
	},
	"background": {
		"scripts": ["function.js", "kyomu.js", "ocw.js", "t2schola.js"]
	}
}
